{
  "buildCommand": "cd ../.. && pnpm prisma:generate && cd src/web && pnpm build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    }
  ],
  "functions": {
    "/api/webhook.js": {
      "maxDuration": 30
    }
  }
}
